          }
        }
      },
      "double-quoted-string":{
        "begin": "\"",
        "end": "\"",
        "name": "string.quoted.double.skill",
        "patterns": [
            {
                "name": "constant.character.escape.skill",
                "match": "\\\\[\"\\\\]"
            }
        ]
      },
      "identifer":{
        "match": "[_a-zA-Z][_a-zA-Z0-9]{0,30}",
        "name": "identifier.identifier"
      }
