            "name": "meta.language.libsl",
            "contentName": "storage.modifier.language.libsl"
          },
          {
            "patterns":[
              {
                "match": "\\b(url)\\b\\s*",
                "name": "keyword.other.libsl.url"
              },
              {
                "include": "#double-quoted-string"
              }
            ],  
            "name": "meta.url.libsl",
            "contentName": "storage.modifier.url.libsl"
          }
        ]
      },
      {
        "include": "#comments-libsldoc"
