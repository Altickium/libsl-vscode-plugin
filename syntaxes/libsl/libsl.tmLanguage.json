      },
      "single-type":{
        "patterns": [
          {
              "begin": "\\b((?:[A-Za-z_]\\w*\\s*\\.\\s*)*)",
              "beginCaptures": {
                "1": {
                  "patterns": [
                    {
                      "match": "[A-Za-z_]\\w*",
                      "name": "storage.type.java"
                    },
                    {
                      "match": "\\.",
                      "name": "punctuation.separator.period.java"
                    }
                  ]
                },
                "name": "meta.libsl.libsl",
                "contentName": "storage.modifier.libsl.libsl",
                "2": {
                  "name": "storage.type.object.array.java"
                }
              },
              "end": "\\s*(;)",
              "patterns": [
