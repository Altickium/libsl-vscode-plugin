    "name": "LibSL",
    "scopeName": "source.libsl",
    "patterns": [
      {
        "begin": "\\b(libsl)\\b\\s*",
        "beginCaptures": {
          "1": {
            "name": "keyword.other.libsl.libsl"
          }
        },
        "end": "\\s*(;)",
        "endCaptures": {
          "1": {
            "name": "punctuation.terminator.libsl"
          }
        },
        "name": "meta.libsl.libsl",
        "contentName": "storage.modifier.libsl.libsl",
        "patterns": [
          {
            "include": "#comments"
          },
          {
            "include": "#double-quoted-string"
          }
        ]
      },
      {
